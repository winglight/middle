# 基础
TZ=America/Los_Angeles

# n8n
N8N_DOMAIN=automations.example.com
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-this-strong-password
N8N_ENCRYPTION_KEY=replace_with_long_random_32+ chars
# 执行队列改用 Redis
EXECUTIONS_MODE=queue

# Redis
REDIS_PASSWORD=change-this-even-stronger

# InfluxDB v2 一次性初始化（首次启动自动创建）
INFLUXDB_ADMIN_USER=admin
INFLUXDB_ADMIN_PASSWORD=ChangeThisAdminPass
INFLUXDB_ORG=my-org
INFLUXDB_BUCKET=my-bucket
INFLUXDB_RETENTION=30d
# 注意：初始化token会保存在容器卷里；也可以添加 INFLUXDB_INIT_ADMIN_TOKEN=... 自定义
# INFLUXDB_INIT_ADMIN_TOKEN=replace_with_long_token  # (可选)

# Cloudflare Tunnel（Token 方式）
CF_TUNNEL_TOKEN=eyJraWQiOiJ...   # 来自 Cloudflare Dashboard 创建tunnel后的 Token
